<UserControl x:Class="emlekmu.AddType"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:emlekmu"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="400">
    
    <Grid>
        <StackPanel Orientation="Vertical">
            <Label x:Name="IdLabel">Id</Label>
            <TextBox x:Name="IdTextBox"></TextBox>
            <Label x:Name="NameLabel">Name</Label>
            <TextBox x:Name="NameTextBox"></TextBox>
            <Label x:Name="DescriptionLabel">Description</Label>
            <TextBox x:Name="DescriptionTextBox"></TextBox>
            <Label x:Name="IconLabel">Icon</Label>
            

        </StackPanel>
    </Grid>
</UserControl>
